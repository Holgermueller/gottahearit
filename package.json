{
  "name": "gottahearit",
  "version": "1.0.0",
  "description": "This project was bootstrapped with [Create React App](https://github.com/facebook/create-react-app).",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon server.js\" \"npm run client\"",
    "client": "cd client &&  npm run start",
    "build": "cd client && npm run build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "install": "cd client && npm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Holgermueller/gottahearit.git"
  },
  "author": "Holger Mueller",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Holgermueller/gottahearit/issues"
  },
  "homepage": "https://github.com/Holgermueller/gottahearit#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.2",
    "colors": "^1.4.0",
    "compression": "^1.7.4",
    "concurrently": "^7.0.0",
    "cors": "^2.8.5",
    "dotenv": "^14.2.0",
    "express": "^4.17.2",
    "express-async-handler": "^1.2.0",
    "if-env": "^1.0.4",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^4.3.1",
    "mongoose": "^6.3.1",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.15"
  }
}
